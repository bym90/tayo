package com.tayo.www.data;

import java.sql.Date;

public class BoardData {
	/*기본게시판*/
	private int no;
	private String id;
	private int category;
	public String getId() {
		return id;
	}
	public int getCategory() {
		return category;
	}
	public void setId(String id) {
		this.id = id;
	}
	public void setCategory(int category) {
		this.category = category;
	}
	private String title;
	private String content;
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	
	private int reno; /*댓글의 부모번호*/
	
	private String password; /*신고 댓글 비밀번호*/
	public int getReno() {
		return reno;
	}
	public void setReno(int reno) {
		this.reno = reno;
	}
	private String writer;
	private Date date;
	private int hit;
	private String ip;
	
	/*답글게시판을 위한*/
	private int group;
	private int step;
	private int order;
	
	/*추천수를 위한*/
	private int good;
	private int	 bad;

	/*파일 업로드를 위한*/
	private String	saveName;
	private String	oriName;
	private String	path;
	private long	length;
	public int getNo() {
		return no;
	}
	public String getTitle() {
		return title;
	}
	public String getPassword() {
		return password;
	}
	public String getWriter() {
		return writer;
	}
	public Date getDate() {
		return date;
	}
	public int getHit() {
		return hit;
	}
	public String getIp() {
		return ip;
	}
	public int getGroup() {
		return group;
	}
	public int getStep() {
		return step;
	}
	public int getOrder() {
		return order;
	}
	public int getGood() {
		return good;
	}
	public int getBad() {
		return bad;
	}
	public String getSaveName() {
		return saveName;
	}
	public String getOriName() {
		return oriName;
	}
	public String getPath() {
		return path;
	}
	public long getLength() {
		return length;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public void setWriter(String writer) {
		this.writer = writer;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	public void setHit(int hit) {
		this.hit = hit;
	}
	public void setIp(String ip) {
		this.ip = ip;
	}
	public void setGroup(int group) {
		this.group = group;
	}
	public void setStep(int step) {
		this.step = step;
	}
	public void setOrder(int order) {
		this.order = order;
	}
	public void setGood(int good) {
		this.good = good;
	}
	public void setBad(int bad) {
		this.bad = bad;
	}
	public void setSaveName(String saveName) {
		this.saveName = saveName;
	}
	public void setOriName(String oriName) {
		this.oriName = oriName;
	}
	public void setPath(String path) {
		this.path = path;
	}
	public void setLength(long length) {
		this.length = length;
	}
	@Override
	public String toString() {
		return "BoardData [no=" + no + ", title=" + title + ", body=" + content + ", password=" + password + ", writer="
				+ writer + ", date=" + date + ", hit=" + hit + ", ip=" + ip + ", group=" + group + ", step=" + step
				+ ", order=" + order + ", good=" + good + ", bad=" + bad + ", saveName=" + saveName + ", oriName="
				+ oriName + ", path=" + path + ", length=" + length + "]";
	}
	
	
}
